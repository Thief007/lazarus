#
#   Makefile.fpc for Lazarus for Free Pascal
#

[package]
name=lazarus-tools
version=0.8a

[target]
dirs=apiwizz

[install]
packagename=tools

[rules]
.PHONY: makefile makefiles

makefile: Makefile.fpc
        -$(FPCMAKE) -w
        
makefiles: makefile
        $(MAKE) -C apiwizz makefiles
