constructor TSplashForm.Create(AOwner: TComponent);
begin
  inherited Create(AOwner);
  Caption := 'Lazarus';
  Position:= poScreenCenter;
  Width := 240;
  Height := 180;
  BorderStyle  := bsToolWindow;

  FBitmap := TBitmap.Create;
  FBitmap.Handle := CreatePixmapIndirect(@SPLASH_IMAGE, ColorToRGB(clBtnFace));

  FTimer := TTimer.Create(self);
  with FTimer do
  begin
    Interval := 500;
    OnTimer := @HideFormTimer;
    Enabled := False;
  end;
end;

destructor TSplashForm.Destroy;
begin
  FBitmap.Free;
  FBitmap:=nil;
  FTimer.Free;
  FTimer:=nil;
  inherited Destroy;
end;

procedure TSplashForm.Click; 
begin
  Hide;
  if FTimer<>nil then begin
    FTimer.Enabled := False;
    //Release resources
    FTimer.Free;
    FTimer:=nil;
    FBitmap.Free;
    FBitmap:=nil;
  end;
end;

procedure TSplashForm.HideFormTimer(Sender : TObject);
begin
  Click;
end;

procedure TSplashForm.Paint;
begin
  inherited Paint;
  if FBitmap <>nil 
  then Canvas.Copyrect(Bounds(0, 0, Width, Height)
     ,FBitmap.Canvas, Rect(0,0, Width, Height));
end;

procedure TSplashForm.StartTimer;
begin
  FTimer.Enabled := True;
end;

end.

