[package]
main=lcl

[require]
packages=rtl fcl

[target]
units=interfaces
implicitunits=winext win32def win32int

[compiler]
options=-gl -S2 -Sh
unitdir=../../units
unittargetdir=../../units/win32

[rules]
cleartarget:
        $(DEL) $(wildcard ../../units/interfaces.*)

all: cleartarget interfaces$(PPUEXT)
